{
  "manifest_version": 2,
  "name": "No Phishing Here",
  "description": "ETHBrno phishing prevention extension based on whitelistning",
  "version": "1.0",
  "homepage_url": "https://github.com/SantiagoDePolonia/no-phishing-here-mvp",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["no-phishing.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/logo-16.png",
      "32": "icons/logo-32.png"
    }
  }
}
